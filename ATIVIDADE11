programa {
  funcao inicio() {

    inteiro nota1, nota2, media
    escreva("Informe suas duas notas (de 0 a 10) ")
    leia(nota1)
    leia(nota2)

    media = (nota1+nota2)/2

    se (media >= 7 e media < 10){
      escreva("Aprovado")
    }

    senao se (media < 7){
      escreva("Reprovado")
    }

    senao se (media == 10){
      escreva("Aprovado com distinção")
    }
    

    
  }
}
